<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="default">
        <class>de.rub.nds.sshstatelearner.db.STLInfo</class>
        <class>de.rub.nds.sshstatelearner.db.QueryResult</class>
        <class>de.rub.nds.sshstatelearner.db.QueryResultResponseFingerprint</class>
        <properties>
            <property name="jakarta.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
            <property name="jakarta.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/stateLearner"/>
            <property name="jakarta.persistence.jdbc.user" value="stateLearner"/>
            <property name="jakarta.persistence.jdbc.password" value="stateLearner"/>
            <property name="jakarta.persistence.schema-generation.database.action" value="none"/>
        </properties>
    </persistence-unit>
</persistence>